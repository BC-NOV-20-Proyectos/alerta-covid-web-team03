<p id="notice"><%= notice %></p>

<h1>Area Histories</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @area_histories.each do |area_history| %>
      <tr>
        <td><%= area_history.date %></td>
        <td><%= area_history.user_id %></td>
        <td><%= link_to 'Show', area_history %></td>
        <td><%= link_to 'Edit', edit_area_history_path(area_history) %></td>
        <td><%= link_to 'Destroy', area_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Area History', new_area_history_path %>
