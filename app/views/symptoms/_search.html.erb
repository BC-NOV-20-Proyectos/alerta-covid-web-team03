<% @symptoms.each do |symptom| %>
  <tr>
    <td><%= symptom.description %></td>
    <td><%= link_to edit_symptom_path(symptom), remote: true do %>
      <i class="far fa-edit"></i>
      <span>Edit</span>
    <% end %> 
    <%= link_to symptom, method: :delete, data: 
    { confirm: 'Are you sure?' } do %>
        <i class="fas fa-user-times"></i>
      <span>Delete</span>
    <% end %>
    </td>
  </tr>
<% end %>
